<span class="octicon octicon-git-merge conversation-item-icon"></span>
<img class="avatar" src="<%- actor.avatar_url %>">
<a class="author" href="<%- actor.html_url %>"><%- actor.login %></a>
merged commit <a href="<%- repository_html_url %>/commit/<%- commit_id %>" class="git-sha"><%- commit_id.substring(0, 8) %></a>
into <span class="git-ref"><%- subject.base.label %></span>
from <span class="git-ref"><%- subject.head.label %></span>
<time is="relative-time" datetime="<%- created_at %>"><%- created_at %></time>
