      <link rel="stylesheet" type="text/css" href="css/app.css" media="all">
      <script type="text/javascript" src="js/app.js"></script>
  </head>
  <body>
    <div id="app" class="container" style="display:none;">
      <input id="toggle-lists" type="checkbox" class="responsive-toggle">
      <div id="lists" class="sidebar pane">
        <header></header>
      </div>
      <div id="threads" class="responsive-content sidebar pane">
        <header></header>
      </div>
      <div id="details" class="pane">
        <header>&nbsp;</header>
      </div>
    </div>
    <div id="shortcuts" style="display:none;">
    </div>

